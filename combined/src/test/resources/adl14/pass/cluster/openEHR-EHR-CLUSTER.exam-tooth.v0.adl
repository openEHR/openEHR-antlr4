definition
	CLUSTER[at0000.1] matches {    -- Examination of a tooth
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001.1] matches {    -- System or structure examined
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::at0.1]    -- Tooth
						}
					}
				}
			}
			ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Tooth label
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.3] occurrences matches {0..1} matches {    -- Presence
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.4,    -- Present
							at0.5]    -- Absent
						}
					}
				}
			}
			ELEMENT[at0012.1] occurrences matches {0..1} matches {    -- Tooth position
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0011.1] occurrences matches {0..*} matches {    -- Structured tooth position
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- No abnormality detected
				value matches {
					DV_BOOLEAN matches {
						value matches {true}
					}
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Clinical description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.6] occurrences matches {0..1} matches {    -- Condition
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.7,    -- Unrestored
							at0.8]    -- Restored.
						}
					}
				}
			}
			ELEMENT[at0.9] occurrences matches {0..1} matches {    -- Gum description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.10] occurrences matches {0..1} matches {    -- Pocket depth
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|0.0..10.0|>
								precision = <|0|>
							>
						>
					>
				}
			}
			CLUSTER[at0.11] occurrences matches {0..*} matches {    -- Findings
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0.12] occurrences matches {0..1} matches {    -- Finding
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.13,    -- Cavity
									at0.14,    -- Amalgam
									at0.15,    -- Gold inlay
									at0.16,    -- Gold foil
									at0.17,    -- Other metal restoration
									at0.18,    -- Composite resin
									at0.19,    -- Traumatic avulsion
									at0.20,    -- Fractured crown
									at0.21,    -- Root tip
									at0.22,    -- Rotated
									at0.23,    -- Root apical filling
									at0.24,    -- Apicoectomy
									at0.25,    -- Intermediate restoration
									at0.26,    -- Crown - temporary
									at0.27,    -- Crown - full
									at0.28,    -- Crown - partial
									at0.29]    -- Crown veneer
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0.30] occurrences matches {0..1} matches {    -- Surface
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.31,    -- Mesial
									at0.32,    -- Distal
									at0.33,    -- Buccal
									at0.34,    -- Lingual
									at0.35,    -- Incisal
									at0.36]    -- Occlusal
								}
							}
						}
					}
					ELEMENT[at0.37] occurrences matches {0..1} matches {    -- Clinical description
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Examination findings
				include
					archetype_id/value matches {/.*/}
			}
			allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Multimedia representation
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v0/}
			}
			ELEMENT[at0006] occurrences matches {0..*} matches {    -- Clinical interpretation
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Examination not done
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
			}
		}
	}


