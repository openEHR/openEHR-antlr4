		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] matches {    -- System or structure examined
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0013,    -- Skin
							at0034,    -- Dermatome
							at0035]    -- Nerve
						}
					}
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {    -- Body site
				value matches {
					DV_TEXT matches {*}
