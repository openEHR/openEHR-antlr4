			ELEMENT[at0.0.15] occurrences matches {0..1} matches {	-- Thickness
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.0.17, 	-- Thick
							at0.0.18, 	-- Thin
							at0.0.19, 	-- Paper-thin
							at0.0.20]	-- Oedematous
