definition
	OBSERVATION[at0000] matches {	-- Fundoscopic examination of eyes
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0136] occurrences matches {0..2} matches {	-- Test Result
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0004] occurrences matches {0..1} matches {	-- Eye examined
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0137, 	-- Left eye
															at0138]	-- Right eye
														}
													}
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- Clinical description
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0053] occurrences matches {0..1} matches {	-- Red reflex
												value matches {
													DV_BOOLEAN matches {
														value matches {True, False; False}
													}
												}
											}
											ELEMENT[at0091] occurrences matches {0..1} matches {	-- Optic disc description
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0090] occurrences matches {0..1} matches {	-- Macula description
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0092] occurrences matches {0..1} matches {	-- Retinal arteries description
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0093] occurrences matches {0..1} matches {	-- Retinal veins description
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0094] occurrences matches {0..1} matches {	-- Retinal background description
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0098] occurrences matches {0..1} matches {	-- Vitreous description
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0143] occurrences matches {0..1} matches {	-- Clinical interpretation
												value matches {
													DV_TEXT matches {*}
												}
											}
											allow_archetype CLUSTER[at0142] occurrences matches {0..*} matches {	-- Multimedia representation
												include
													archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v0/}
											}
											allow_archetype CLUSTER[at0139] occurrences matches {0..*} matches {	-- Additional findings
												include
													archetype_id/value matches {/openEHR-EHR-CLUSTER\.clinical_findings(-[a-zA-Z0-9_]+)*\.v1/}
												exclude
													archetype_id/value matches {/.*/}
											}
											allow_archetype CLUSTER[at0141] occurrences matches {0..*} matches {	-- Examination not done
												include
													archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
											}
											ELEMENT[at0126] occurrences matches {0..*} matches {	-- Comment
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0013] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0140] occurrences matches {0..1} matches {	-- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {	-- Mydriatic used
										value matches {
											DV_BOOLEAN matches {
												value matches {True, False}
											}
										}
									}
									ELEMENT[at0054] occurrences matches {0..1} matches {	-- Small pupil
										value matches {
											DV_BOOLEAN matches {
												value matches {True, False; False}
											}
										}
									}
									ELEMENT[at0124] occurrences matches {0..1} matches {	-- Uncooperative patient
										value matches {
											DV_BOOLEAN matches {
												value matches {True, False; False}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0028] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0029] occurrences matches {0..1} matches {	-- Method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0030, 	-- Direct
									at0031, 	-- Indirect
									at0131, 	-- Contact lens biomicroscopy
									at0132, 	-- Non-contact lens biomicroscopy
									at0129, 	-- Mydriatic retinography
									at0130, 	-- Non-mydriatic retinography
									at0135]	-- Angiography
								}
							}
						}
					}
					allow_archetype CLUSTER[at0089] occurrences matches {0..*} matches {	-- Device details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0067] occurrences matches {0..1} matches {	-- Field angle
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0068, 	-- 30º
									at0069, 	-- 45º
									at0122, 	-- 60º
									at0070, 	-- 100º
									at0071]	-- 200º
								}
							}
						}
					}
					ELEMENT[at0072] occurrences matches {0..1} matches {	-- Attempts
						value matches {
							DV_COUNT matches {
								magnitude matches {|1..3|}
							}
						}
					}
					CLUSTER[at0073] occurrences matches {0..1} matches {	-- Subdivision of the retina
						name matches {
							DV_CODED_TEXT matches {
								defining_code matches {[ac0001]}		-- Subdivision of retina
							}
						}
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0074] occurrences matches {0..*} matches {	-- ETRDS fields
								name matches {
									DV_CODED_TEXT matches {
										defining_code matches {[ac0002]}		-- Diabetic Retinopathy Study field
									}
								}
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0075, 	-- Study field 1
											at0076, 	-- Study field 2
											at0077, 	-- Study field 3
											at0078, 	-- Study field 4
											at0079, 	-- Study field 5
											at0080, 	-- Study field 6
											at0081]	-- Study field 7
										}
									}
								}
							}
							ELEMENT[at0082] occurrences matches {0..*} matches {	-- Mosaic and peripherals
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0083, 	-- Mosaic
											at0084, 	-- Central
											at0085, 	-- Nasal
											at0086, 	-- Temporal
											at0087, 	-- Superior
											at0088]	-- Inferior
										}
									}
								}
							}
						}
					}
					ELEMENT[at0056] occurrences matches {0..1} matches {	-- Shadow artifact
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False; False}
							}
						}
					}
					ELEMENT[at0055] occurrences matches {0..1} matches {	-- Cataract artifact
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False; False}
							}
						}
					}
					ELEMENT[at0059] occurrences matches {0..1} matches {	-- Quality of visualisation
						value matches {
							1|[local::at0060], 	-- Quality inadequate for any diagnostic purpose
							2|[local::at0061], 	-- Unable to exclude all emergent findings
							3|[local::at0062], 	-- Only able to exclude emergent findings
							4|[local::at0063], 	-- Quality not ideal, but is possible to exclude subtle findings
							5|[local::at0064]  	-- Ideal quality
						}
					}
					ELEMENT[at0058] occurrences matches {0..1} matches {	-- Visualisation description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0123] occurrences matches {0..1} matches {	-- High refraction
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False; False}
							}
						}
					}
					allow_archetype CLUSTER[at0144] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	terminologies_available = <"SNOMED-CT", ...>
	term_definitions = <
		["es"] = <
			items = <
				["at0000"] = <
					text = <"*Fundoscopic examination of eyes(en)">
					description = <"*Record of clinical findings on fundoscopy of eyes.(en)">
				>
				["at0001"] = <
					text = <"*Event Series(en)">
					description = <"*@ internal @(en)">
				>
				["at0002"] = <
					text = <"*Any event(en)">
					description = <"*Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.(en)">
				>
				["at0003"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0004"] = <
					text = <"*Eye examined(en)">
					description = <"*Identification of the eye which is being examined.(en)">
					comment = <"*Matches to DICOM Laterality (0020,0060) attribute.(en)">
				>
				["at0008"] = <
					text = <"*Clinical description(en)">
					description = <"*Narrative description of the overall findings observed during the physical examination of the eye examined.(en)">
				>
				["at0013"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0014"] = <
					text = <"*Mydriatic used(en)">
					description = <"*True if mydriatic is used.(en)">
				>
				["at0028"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0029"] = <
					text = <"*Method(en)">
					description = <"*Method chosen to perform the funduscopic examination.(en)">
				>
				["at0030"] = <
					text = <"*Direct(en)">
					description = <"*Study performed by direct ophthalmoscopy.(en)">
				>
				["at0031"] = <
					text = <"*Indirect(en)">
					description = <"*Study of eye fundus by indirect ophthalmoscopy method.(en)">
				>
				["at0053"] = <
					text = <"*Red reflex(en)">
					description = <"*True if Red Reflex is present.(en)">
				>
				["at0054"] = <
					text = <"*Small pupil(en)">
					description = <"*True if during the acquisition, pupil diameter is smaller than normal (3,3mm).(en)">
				>
				["at0055"] = <
					text = <"*Cataract artifact(en)">
					description = <"*True if cataract obstructs the visualization of eye fundus.(en)">
				>
				["at0056"] = <
					text = <"*Shadow artifact(en)">
					description = <"*True if shadow artifact is present on the border of the image.(en)">
				>
				["at0058"] = <
					text = <"*Visualisation description(en)">
					description = <"*Narrative description about visualisation.(en)">
				>
				["at0059"] = <
					text = <"*Quality of visualisation(en)">
					description = <"*Levels quantifying the quality of each acquisition, based in the ease to visualize the structures on the eye fundus.(en)">
				>
				["at0060"] = <
					text = <"Inadecuada para propósitos diagnósticos">
					description = <"La calidad de la imagen no alcanza los criterios mínimos para diagnóstico">
				>
				["at0061"] = <
					text = <"Incapacidad de distinguir todos los hallazgos fundamentales">
					description = <"En la imagen se aprecia la estructura del fondo de ojo, sin embargo, no es posible distinguir hallazgos clínicos">
				>
				["at0062"] = <
					text = <"Sólo es posible distinguir los hallazgos fundamentales">
					description = <"Es posible detectar signos útiles para diagnóstico, pero se escapan los mas sutiles">
				>
				["at0063"] = <
					text = <"La calidad no es ideal, pero es posible identificar algunos hallazgos sutiles">
					description = <"Es posible identificar hallazgos sutiles de interés diagnóstico, pero la calidad de imagen aún podría ser mejor. Suele darse este caso en imágenes periféricas o que implican dificultad, que han sido capturadas correctamente.">
				>
				["at0064"] = <
					text = <"Calidad ideal">
					description = <"Imagen ideal en la cual es sencillo identificar hallazgos de interés clínico. Suele darse este caso en imágenes directas obtenidas correctamente.">
				>
				["at0067"] = <
					text = <"*Field angle(en)">
					description = <"*Describes the optical acceptance angle of the lens used during the test.(en)">
				>
				["at0068"] = <
					text = <"*30º(en)">
					description = <"*30º angle used for small pupil (SP) capture (for patients with pupil diameter <3.3mm).(en)">
				>
				["at0069"] = <
					text = <"*45º(en)">
					description = <"*45º angle used to acquire eye fundus of normal pupils.(en)">
				>
				["at0070"] = <
					text = <"*100º(en)">
					description = <"*Wide angle acquisition.(en)">
				>
				["at0071"] = <
					text = <"*200º(en)">
					description = <"*Ultra-wide angle acquisition.(en)">
				>
				["at0072"] = <
					text = <"*Attempts(en)">
					description = <"*Number of attempts before obtaining the acquisition (doesn't compute if test is repeated by a specific recognized technical failure).(en)">
				>
				["at0073"] = <
					text = <"*Subdivision of the retina(en)">
					description = <"*Subdivision of the retina identifying eye fundus image locations.(en)">
				>
				["at0074"] = <
					text = <"*ETRDS fields(en)">
					description = <"*Subdivision of the retina based on Diabetic Retinopathy Study fields.(en)">
				>
				["at0075"] = <
					text = <"Campo de estudio 1">
					description = <"**(en)">
				>
				["at0076"] = <
					text = <"Campo de estudio 2">
					description = <"**(en)">
				>
				["at0077"] = <
					text = <"Campo de estudio 3">
					description = <"**(en)">
				>
				["at0078"] = <
					text = <"Campo de estudio 4">
					description = <"**(en)">
				>
				["at0079"] = <
					text = <"Campo de estudio 5">
					description = <"**(en)">
				>
				["at0080"] = <
					text = <"Campo de estudio 6">
					description = <"**(en)">
				>
				["at0081"] = <
					text = <"Campo de estudio 7">
					description = <"**(en)">
				>
				["at0082"] = <
					text = <"*Mosaic and peripherals(en)">
					description = <"*Division of the retina in quadrants + mosaic obtained from the combination of them.(en)">
				>
				["at0083"] = <
					text = <"*Mosaic(en)">
					description = <"*Mosaic obtained from combining every peripheral acquisitions and the center one.(en)">
				>
				["at0084"] = <
					text = <"*Central(en)">
					description = <"*Image centered on the macula.(en)">
				>
				["at0085"] = <
					text = <"*Nasal(en)">
					description = <"*Image centered on the optic nerve or papila.(en)">
				>
				["at0086"] = <
					text = <"*Temporal(en)">
					description = <"*Image centered on the temporal quadrant of the retina.(en)">
				>
				["at0087"] = <
					text = <"*Superior(en)">
					description = <"*Image centered on the superior half of the retina.(en)">
				>
				["at0088"] = <
					text = <"*Inferior(en)">
					description = <"*Image centered on the inferior half of the retina.(en)">
				>
				["at0089"] = <
					text = <"*Device details(en)">
					description = <"*Details of the device used to acquire eye fundus images.(en)">
				>
				["at0090"] = <
					text = <"*Macula description(en)">
					description = <"*Narrative description about the macula.(en)">
				>
				["at0091"] = <
					text = <"*Optic disc description(en)">
					description = <"*Narrative description about the optic disc.(en)">
				>
				["at0092"] = <
					text = <"*Retinal arteries description(en)">
					description = <"*Narrative description about the retinal arteries.(en)">
				>
				["at0093"] = <
					text = <"*Retinal veins description(en)">
					description = <"*Narrative description about the retinal veins.(en)">
				>
				["at0094"] = <
					text = <"*Retinal background description(en)">
					description = <"*Narrative description about the retinal background.(en)">
				>
				["at0098"] = <
					text = <"*Vitreous description(en)">
					description = <"*Narrative description about the vitreous humour.(en)">
				>
				["at0122"] = <
					text = <"*60º(en)">
					description = <"*60º angle used to acquire eye fundus in some DR screening studies.(en)">
				>
				["at0123"] = <
					text = <"*High refraction(en)">
					description = <"*True if the refraction of the eye exceeds the range from -12D to +15D.(en)">
				>
				["at0124"] = <
					text = <"*Uncooperative patient(en)">
					description = <"*True if patient doesn't collaborate during the image acquisition.(en)">
				>
				["at0126"] = <
					text = <"*Comment(en)">
					description = <"*Additional narrative about the physical examination findings, not captured in other fields.(en)">
				>
				["at0129"] = <
					text = <"*Mydriatic retinography(en)">
					description = <"*Observation of retina through funduscopic images acquired by previous dilatation of patient's pupils.(en)">
				>
				["at0130"] = <
					text = <"*Non-mydriatic retinography(en)">
					description = <"*Observation of retina through funduscopic images acquired without previous dilatation of patient's pupils.(en)">
				>
				["at0131"] = <
					text = <"*Contact lens biomicroscopy(en)">
					description = <"*Eye fundus viewing through biomicroscopy lens in contact to patient's eye surface.(en)">
				>
				["at0132"] = <
					text = <"*Non-contact lens biomicroscopy(en)">
					description = <"*Eye fundus viewing through biomicroscopy lens without contact to patient's eye surface.(en)">
				>
