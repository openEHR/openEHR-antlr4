					comment = <"For example, maternal fever or exertion.">
				>
				["at0020"] = <
					text = <"Device">
					description = <"Details about the device used to detect the fetal heart rate.">
					comment = <"For example, Pinard's stethoscope or Doppler Ultrasound.">
				>
