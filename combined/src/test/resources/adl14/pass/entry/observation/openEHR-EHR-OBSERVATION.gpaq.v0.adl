												value matches {PTHM/|PT0M..PT24H|}
											}
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {    -- 13. Leisure - Moderate activity?
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
