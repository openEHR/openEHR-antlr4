abstract class ACTIVITY
    is_a LOCATABLE

feature_group ("access")

    |
    | Structured description of the Activity.
    |
    prop description : ITEM_STRUCTURE ;
	
    |
    | Computable representation of timing information.
    |
    prop timing :? DV_PARSABLE ;

    |
    | Computable representation of timing information.
    |
    prop action_archetype_id : String ;

end

