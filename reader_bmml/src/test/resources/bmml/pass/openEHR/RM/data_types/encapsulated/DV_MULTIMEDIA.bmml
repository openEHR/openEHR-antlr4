import org.openEHR.base.foundation_types ;


class DV_MULTIMEDIA
    is_a DV_ENCAPSULATED

feature_group("access")

    prop alternate_text :? String;

    prop uri :? DV_URI;

    prop data : array<Byte>;

    prop integrity_check :? array<Byte>;

    prop media_type : Terminology_code «iana.media-types»;

    prop compression_algorithm :? Terminology_code «iana.compression-algorithms»;

    prop integrity_check_algorithm :? Terminology_code «iana.integrity-check-algorithms»;

    prop thumbnail :? Multimedia;

    prop size : Integer;

end

