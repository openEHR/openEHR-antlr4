class OBJECT_REF
    is_a Any

feature_group("infrastructure")

    |
    | Optional namespace in which target object id can be resolved
    |
    prop namespace :? String ;

    |
    | Machine identifier of target object.
    |
    prop id : OBJECT_ID ;

    |
    | Typename of object in current type system.
    | TODO: determine if anything is allowed here - probably needs to be
    |
    prop type : String ;

end




